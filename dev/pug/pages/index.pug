extends ../layout/main

block head
  - var title = 'Главная'
  - var bodyClass = ''
block content
  section.swiper-container.main-slider
    .swiper-wrapper
      .swiper-slide(style="background-image: url(https://picsum.photos/1920/900)")
        .container
          .row
            .col-12
